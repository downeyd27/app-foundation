<% content_for :title do %>Sign up<% end %>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <section class="debug">
      <h1>Sign up</h1>
      <h2>Here you can signup for this amazing application!</h2>
      <%= form_for @user, html: { role: "form" } do |f| %>
        <%= render 'fields', f: f %>
        <%= f.submit "Create my account", class: "create_user_submit btn btn-primary" %>
      <% end %>
    </section>
  </div>
</div>