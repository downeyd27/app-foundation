<%= content_for :title do %>All users<% end %>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1>All users</h1>

    <%= will_paginate %>

    <ul class="users">
      <% @users.each do |user| %>
        <li>
          <%= gravatar_for user, size: 50 %>
          <%= link_to user.name, user %>
        </li>
      <% end %>
    </ul>

    <%= will_paginate %>

  </div>
</div>